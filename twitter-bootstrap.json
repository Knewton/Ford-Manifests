{
    "*": {
        "uri": "http://twitter.github.io/bootstrap/2.3.2/assets/bootstrap.zip",
        "packaged": "zip",
        "root": "bootstrap"
    },
    "twitter-bootstrap": {
        "comp": {
            "js": "js/bootstrap",
            "css": "css/bootstrap",
            "images": [
                "img/glyphicons-halflings.png",
                "img/glyphicons-halflings-white.png"
            ]
        },
        "cssImageFix": "../img",
        "reqs": ["jquery"]
    },
    "responsive": {
        "comp": {
            "css": "css/bootstrap-responsive"
        },
        "reqs": {
            "&": ["twitter-bootstrap"]
        }
    }
}
